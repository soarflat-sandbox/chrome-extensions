{
  "name": "Message Counter",
  "version": "1.3",
  "description": "content scriptsにメッセージを送信して一定のカウントに達したら処理を停止",
  "content_scripts": [
    {
      "matches": ["https://*/*"],
      "js": ["page.js"]
    }
  ],
  "browser_action": {
    "default_title": "Time to current page",
    "default_icon": "clock.png",
    "default_popup": "popup.html"
  },
  "manifest_version": 2
}
